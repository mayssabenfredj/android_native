<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".application_list"
    android:background="@color/bleu"
    android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:contentDescription="@string/image"
                app:srcCompat="@drawable/menu" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="66dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppinsregular"
                android:text="@string/parental_control"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:contentDescription="@string/image"
                app:srcCompat="@drawable/usericone" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="310dp"
            android:layout_height="35dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/search_background"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView34"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/search"
                android:contentDescription="@string/image"/>

            <TextView
                android:id="@+id/textView42"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="20dp"
            android:background="#97FDFDFD" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="36dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView70"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/googleicone"
                android:contentDescription="@string/image" />

            <TextView
                android:id="@+id/textView57"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:alpha="0.86"
                android:fontFamily="@font/abelregular"
                android:text="@string/google_chrome"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView58"
                android:layout_width="14dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="7dp"
                android:layout_weight="1"
                android:alpha="0.86"
                android:fontFamily="@font/abelregular"
                android:text="@string/on"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <ImageView
                android:id="@+id/imageView37"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:layout_marginStart="-25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/on"
                android:contentDescription="@string/image"/>
        </LinearLayout>

        <View
            android:id="@+id/view2"
            android:layout_width="350dp"
            android:layout_height="2dp"
            android:layout_marginStart="25dp"
            android:background="#08FDFDFD" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="36dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/cameraicon"
                android:contentDescription="@string/image" />

            <TextView
                android:id="@+id/textView64"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:alpha="0.86"
                android:fontFamily="@font/abelregular"
                android:text="@string/camera"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView65"
                android:layout_width="14dp"
                android:layout_height="20dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="7dp"
                android:layout_weight="1"
                android:alpha="0.86"
                android:fontFamily="@font/abelregular"
                android:text="@string/off"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <ImageView
                android:id="@+id/imageView80"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:layout_marginStart="-20dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/of"
                android:contentDescription="@string/image" />
        </LinearLayout>

        <View
            android:id="@+id/view3"
            android:layout_width="350dp"
            android:layout_height="2dp"
            android:layout_marginStart="25dp"
            android:background="#08FDFDFD" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="460dp"
            android:alpha="0.59"
            android:background="@color/white"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="94dp"
                android:layout_height="40dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="15dp"
                android:orientation="vertical"
                android:paddingBottom="5dp"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@+id/imageView33"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:alpha="1"
                    android:contentDescription="@string/image"
                    app:srcCompat="@drawable/home_bleu" />

                <TextView
                    android:id="@+id/textView60"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:letterSpacing="0.2"
                    android:text="@string/dashboard"
                    android:textAlignment="center"
                    android:textColor="@color/bleu"
                    android:textSize="10sp"
                    tools:ignore="SmallSp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="94dp"
                android:layout_height="40dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/bleushape"
                android:orientation="vertical"
                android:paddingBottom="5dp"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@+id/imageView57"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:alpha="1"
                    android:contentDescription="@string/image"
                    app:srcCompat="@drawable/settings_white" />

                <TextView
                    android:id="@+id/textView61"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:letterSpacing="0.2"
                    android:text="@string/control"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="10sp"
                    tools:ignore="SmallSp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="92dp"
                android:layout_height="40dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="15dp"
                android:orientation="vertical"
                android:paddingBottom="5dp"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@+id/imageView58"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:alpha="1"
                    android:contentDescription="@string/image"
                    app:srcCompat="@drawable/media" />

                <TextView
                    android:id="@+id/textView62"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:letterSpacing="0.2"
                    android:text="@string/social_media"
                    android:textAlignment="center"
                    android:textColor="@color/bleu"
                    android:textSize="10sp"
                    tools:ignore="SmallSp,TooManyViews" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="92dp"
                android:layout_height="40dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="15dp"
                android:orientation="vertical"
                android:paddingBottom="5dp"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@+id/imageView59"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:alpha="1"
                    android:contentDescription="@string/image"
                    app:srcCompat="@drawable/alert" />

                <TextView
                    android:id="@+id/textView48"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:letterSpacing="0.2"
                    android:text="@string/alerts"
                    android:textAlignment="center"
                    android:textColor="@color/bleu"
                    android:textSize="10sp"
                    tools:ignore="SmallSp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>